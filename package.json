{
  "name": "docker-node-boilerplate",
  "version": "0.0.1",
  "description": "docker下部署node.js应用 ( Babel, ES2015, ESLint, Dockerfile... )",
  "main": "lib/server.js",
  "scripts": {
    "start": "node lib/server.js",
    "build": "npm run clean && cross-env BABEL_ENV=commonjs babel src --out-dir lib && npm run compress",
    "compress": "gulp compress",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihahoo/docker-node-boilerplate.git"
  },
  "keywords": [
    "docker",
    "node"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ihahoo/docker-node-boilerplate/issues"
  },
  "homepage": "https://github.com/ihahoo/docker-node-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-uglify": "^2.0.0",
    "pump": "^1.0.1",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "express": "^4.14.0"
  }
}
